{
  "expo": {
    "name": "Yapay Zeka Aile Rehberi",
    "slug": "AIFamilyApp",
    "scheme": "aifamilyapp",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#193140"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.aifamilyapp.app",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#193140"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "aifamilyapp",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "23c6927f-a802-41f7-b03a-561016f24f56"
      }
    }
  }
}
